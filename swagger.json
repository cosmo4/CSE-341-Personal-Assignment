{
    "swagger": "2.0",
    "info": {
      "title": "My Contacts API",
      "description": "Use this to access my list of contacts",
      "version": "1.0.0"
    },
    "host": "cse-341-personal-assignment-gnjj.onrender.com",
    "basePath": "/",
    "schemes": [
      "https"
    ],
    "paths": {
      "/contacts/": {
        "get": {
          "description": "get all contacts",
          "produces": [
            "application/json"
          ],
          "responses": {
            "200": {
              "description": "OK"
            }
          }
        },
        "post": {
          "description": "create a new contact",
          "parameters": [
            {
              "name": "body",
              "in": "body",
              "schema": {
                "type": "object",
                "properties": {
                  "firstName": {
                    "example": "First Name"
                  },
                  "lastName": {
                    "example": "Last Name"
                  },
                  "email": {
                    "example": "example@gmail.com"
                  },
                  "favoriteColor": {
                    "example": "color"
                  },
                  "birthday": {
                    "example": "01-01-2000"
                  }
                }
              }
            }
          ],
          "responses": {
            "201": {
              "description": "Created"
            },
            "500": {
              "description": "Internal Server Error"
            }
          }
        }
      },
      "/contacts/{id}": {
        "get": {
          "description": "get a contact by id",
          "produces": [
            "application/json"
          ],
          "parameters": [
            {
              "name": "id",
              "in": "path",
              "required": true,
              "type": "string"
            }
          ],
          "responses": {
            "200": {
              "description": "OK"
            },
            "404": {
              "description": "Not Found"
            },
            "500": {
              "description": "Internal Server Error"
            }
          }
        },
        "put": {
          "description": "update a contact's info",
          "parameters": [
            {
              "name": "id",
              "in": "path",
              "required": true,
              "type": "string"
            }
          ],
          "responses": {
            "204": {
              "description": "No Content"
            },
            "500": {
              "description": "Internal Server Error"
            }
          }
        },
        "delete": {
          "description": "delete a contact",
          "parameters": [
            {
              "name": "id",
              "in": "path",
              "required": true,
              "type": "string"
            }
          ],
          "responses": {
            "200": {
              "description": "OK"
            },
            "500": {
              "description": "Internal Server Error"
            }
          }
        }
      }
    }
  }